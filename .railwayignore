# Railway deployment ignore file

# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Development files (do NOT ignore src/ for Railway builds)
# Keep sources so Railway can run `npm run build`
# If you ever commit built output, you can ignore dist/ locally, but
# Railway will build from source, so include everything needed.

# Environment files (will be set in Railway dashboard)
.env
.env.local
.env.development
.env.test

# IDE files
.vscode/
.idea/
*.swp
*.swo

# Logs
logs/
*.log

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/

# Dependency directories
node_modules/
jspm_packages/

# Optional npm cache directory
.npm

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# parcel-bundler cache (https://parceljs.org/)
.cache
.parcel-cache

# next.js build output
.next

# nuxt.js build output
.nuxt

# vuepress build output
.vuepress/dist

# Serverless directories
.serverless

# Test files
test/
tests/
__tests__/

# Documentation
docs/
*.md
!README.md

# Git
.git/
.gitignore

# Temporary files
tmp/
temp/
